    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) ARM Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/2025/three/advent",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/2025/three",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "targetArchitecture": "arm64"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/2025/three/advent",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/2025/three",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },        {
            "name": "(gdb) WINDOWS LAUNCH",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/twelve/advent.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/twelve/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
